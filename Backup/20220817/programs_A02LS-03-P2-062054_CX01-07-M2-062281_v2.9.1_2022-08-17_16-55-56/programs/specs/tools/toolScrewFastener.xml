<?xml version="1.0"?>
<!-- ===========================================================================
 tool_name: tool name 
 tool_type: tool type is defined in string format 
 tcp_position    : tcp position in tool coordinate [m]
 tcp_orientation : [w; x; y; z] quaternion in tool coordinate
 mass : kg
 inertia : [ Ixx Iyy Izz Ixy Ixz Iyz ] at the center of mass (tool coordinate)
 center_of_mass : [m] in tool coordinate
  
 graphics/position_in_parent : posiiton in parent coordinate [m]
 graphics/orientation_in_parent : [w; x; y; z] quaternion in parent coordinate 
 =========================================================================== -->
<fvr>
    <tool_name>ScrewFastener</tool_name>
    <tool_type>TOOL_RIGIDBODY</tool_type>
    <tcp>
        <tcp_position>0.0 0.0 0.2</tcp_position>
        <tcp_orientation>0.9238795  0.0  0.3826834  0.0</tcp_orientation>
    </tcp>
    <mass>3.314</mass>
    <inertia>0.007 0.021 0.002 -0.002 0.008 -0.001</inertia>
    <center_of_mass>-0.096573 -0.017668 0.039263</center_of_mass>
    <graphics>
        <obj_file>
            <path>graphics/cube.obj</path>
            <position_in_parent>0.0 0.0 0.1</position_in_parent>
            <orientation_in_parent>1 0 0 0</orientation_in_parent>
            <scaling>0.02 0.02 0.2</scaling>
        </obj_file>
    </graphics>
</fvr>
