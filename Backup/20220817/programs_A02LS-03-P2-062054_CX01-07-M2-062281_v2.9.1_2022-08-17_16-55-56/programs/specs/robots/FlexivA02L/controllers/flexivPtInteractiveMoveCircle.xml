<fvr>
    <primitive>
        <primitive_name> interactive_move_circle </primitive_name>
        <primitive_type> INTERACTIVE_MOVE_CIRCLE </primitive_type>
        <robot_type>Flexiv</robot_type>
        <param_configs>PtInteractiveMoveCircle.xml</param_configs>
        <task>
            <priority> 0 </priority>
            <task_name>JointConstraint</task_name>
            <task_type>TASK_JOINTCONSTRAINT</task_type>
            <dof>7</dof>
            <kp>200 200  200  300  900  1000  1000</kp>
            <kv>15 15 15 30 40 60 60</kv>
        </task>
        <task>
            <priority> 1 </priority>
            <task_name>PosOriControl</task_name>
            <task_type>TASK_POS_ORI</task_type>
            <link_name>flange</link_name>
            <pos_in_parent>0.0 0.0 0.0</pos_in_parent>
            <dof>6</dof>
            <kp>1000 1000 1000 3000 3000 3000</kp>
            <kv>50 50 50 50 50 50 </kv>
            <ki>2.0 2.0 2.0  0.015  0.015 0.015 </ki>
            <max_iForce> 15.0 15.0 15.0  5.0  5.0 5.0</max_iForce>
            <friction_compensation> 0 </friction_compensation>
        </task>
        <task>
            <priority> 2 </priority>
            <task_name>PostureControl</task_name>
            <task_type>TASK_POSTURE</task_type>
            <dof>7</dof>
            <kp>150  150  150  150  180  180  180</kp>
            <kv>60   50   60   50   60   50   60</kv>
        </task>
    </primitive>
</fvr>
