<?xml version="1.0"?>
<!-- ==========================================================================
 primitive type options :
 "INIT", "MOVE", "READY", "CONTACT", "SLIDE", "SLIDE_ADAPTIVE", "SLIDE_SPIRAL",
 "SLIDE_SINEWAVE", "MAGNET", "PEGINHOLE"

robot type options : Flexiv, UR5, Bullet, Klampt
 ===========================================================================-->
<fvr>
    <primitive>
        <primitive_name> nullspaceTest </primitive_name>
        <primitive_type> NULLSPACE_TEST </primitive_type>
        <robot_type>Bullet</robot_type>
        <param_configs>PtNullspaceTest.xml</param_configs>
        <task>
            <priority> 0 </priority>
            <task_name>PosOriControl</task_name>
            <task_type>TASK_POS_ORI_IMPEDANCE</task_type>
            <link_name>flange</link_name>
            <pos_in_parent>0.0 0.0 0.0</pos_in_parent>
            <dof>6</dof>
            <kp>100 100 100 10 10 10</kp>
            <!-- Note: kv in this task controller is used as damping ratio-->
            <kv>0.7 0.7 0.7 0.7 0.7 0.7</kv>
        </task>
        <task>
            <priority> 1 </priority>
            <task_name>PostureControl</task_name>
            <task_type>TASK_POSTURE_BLENDING</task_type>
            <dof>7</dof>
            <kp>1600  1600  1600  1600  1600  1600  1600</kp>
            <kv>60  60  60  60   60   60  60</kv>
        </task>
    </primitive>
</fvr>
