<?xml version="1.0"?>
<!-- ==========================================================================
 primitive type options :
 "INIT", "MOVE", "READY", "CONTACT", "SLIDE", "SLIDE_ADAPTIVE", "SLIDE_SPIRAL",
 "SLIDE_SINEWAVE", "MAGNET", "PEGINHOLE"

robot type options : Flexiv, UR5, Bullet, Klampt
 ===========================================================================-->
<fvr>
    <primitive>
        <!-- ===================================================================
         input parameter configuration

         par_type : "CART_POS", "QUAT", "POSE", "BOOL", "DOUBLE", "INT", "STRING"
         par_input_type : "KEYIN", "TEACH" , "PT"
         ====================================================================-->
        <input_par_config>
            <par_name> tuneDOF </par_name>
            <par_desc> "Cartesian DOF to tune " </par_desc>
            <par_type> INT </par_type>
            <par_unit> none </par_unit>
            <par_required> 1 </par_required>
            <par_default> 0 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> stepAmp </par_name>
            <par_desc> "amplitude of test command signal" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m </par_unit>
            <par_required> 1 </par_required>
            <par_default> 0 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> twiddleMinDelta </par_name>
            <par_desc> "min Twiddle step threshold" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 1 </par_required>
            <par_default> 0.01 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> twiddleDeltaKp </par_name>
            <par_desc> "Twiddle Kp initial step" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 1 </par_required>
            <par_default> 1.0 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> twiddleDeltaKv </par_name>
            <par_desc> "Twiddle Kv initial step" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 1 </par_required>
            <par_default> 1.0 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> twiddleDeltaKi </par_name>
            <par_desc> "Twiddle Ki initial step" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 1 </par_required>
            <par_default> 1.0 </par_default>
        </input_par_config>
        <!-- ===================================================================
         state parameter configuration
         ====================================================================-->
    </primitive>
</fvr>
