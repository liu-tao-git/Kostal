<?xml version="1.0"?>
<!-- ==========================================================================
 primitive type options :
 "INIT", "MOVE", "READY", "CONTACT", "SLIDE", "SLIDE_ADAPTIVE", "SLIDE_SPIRAL",
 "SLIDE_SINEWAVE", "MAGNET", "PEGINHOLE"

robot type options : Flexiv, UR5, Bullet, Klampt
 ===========================================================================-->
<fvr>
    <primitive>
        <pt_desc> Executing composed move command in hybrid mode: MOVL, MOVC and MOVS </pt_desc>
        <!-- ===================================================================
         input parameter configuration

         par_type : "CART_POS", "QUAT", "POSE", "BOOL", "DOUBLE", "INT", "STRING"
         par_input_type : "KEYIN", "TEACH", "PT"
         ====================================================================-->
        <input_par_config>
            <par_name> contactLinearVel </par_name>
            <par_desc> target linear velocity for polishing</par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m/s </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 500.0 </par_max>
            <par_min> 0.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> contactForce </par_name>
            <par_desc> target force value for polishing</par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> N </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 40.0 </par_max>
            <par_min> -40.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> contactAxisForce </par_name>
            <par_desc> the Tcp axis for force control</par_desc>
            <par_type> STRING </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> z </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> contactCOPOne </par_name>
            <par_desc> target COP value for polishing</par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 1.0 </par_max>
            <par_min> -1.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> contactAxisComplianceOne </par_name>
            <par_desc> the Tcp axis for surface following control</par_desc>
            <par_type> STRING </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> Rx </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> contactCurvatureRadiusOne </par_name>
            <par_desc> range of radius of curvature for the compliance axis</par_desc>
            <par_type> STRING </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> small </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> contactCOPTwo </par_name>
            <par_desc> target COP value for polishing</par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 1.0 </par_max>
            <par_min> -1.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> contactAxisComplianceTwo </par_name>
            <par_desc> the Tcp axis for surface following control</par_desc>
            <par_type> STRING </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> Ry </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> contactCurvatureRadiusTwo </par_name>
            <par_desc> range of radius of curvature for the compliance axis</par_desc>
            <par_type> STRING </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> small </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> areaPathPattern </par_name>
            <par_desc> use pattern or trajectory file </par_desc>
            <par_type> BOOL </par_type>
            <par_required> 0 </par_required>
            <par_default> 1 </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> areaPathType </par_name>
            <par_desc> path type, square or spiral</par_desc>
            <par_type> STRING </par_type>
            <par_required> 0 </par_required>
            <par_default> rectangle </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> fileName </par_name>
            <par_desc> trajectory file name </par_desc>
            <par_type> STRING </par_type>
            <par_required> 0 </par_required>
            <par_default> areaFileName </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> areaEnableOverWrite </par_name>
            <par_desc> enable force and velocity overwrite </par_desc>
            <par_type> BOOL </par_type>
            <par_required> 0 </par_required>
            <par_default> 0 </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> rectPositionOne </par_name>
            <par_desc> the first coordinate for the rectangular path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> rectPositionTwo </par_name>
            <par_desc> the second coordinate for the rectangular path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> rectPositionThree </par_name>
            <par_desc> the third coordinate for the rectangular path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> rectBlendZone </par_name>
            <par_desc> blend zone of the rectangular path </par_desc>
            <par_type> STRING </par_type>
            <par_required> 0 </par_required>
            <par_default> Z0 </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> rectSpacing </par_name>
            <par_desc> line spacing of the rectangular path </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 1.0 </par_max>
            <par_min> 0.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> spiralPositionOne </par_name>
            <par_desc> the first coordinate for the spiral path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> spiralPositionTwo </par_name>
            <par_desc> the second coordinate for the spiral path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> spiralPositionThree </par_name>
            <par_desc> the third coordinate for the spiral path </par_desc>
            <par_type> COORD </par_type>
            <par_unit> m-deg </par_unit>
            <par_option>TRAJ::START WORLD::WORLD_ORIGIN</par_option>
            <par_required> 0 </par_required>
            <par_default> 0 0 0 0 0 0 WORLD WORLD_ORIGIN</par_default>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> spiralCloseWise </par_name>
            <par_desc> blend zone of the spiral path </par_desc>
            <par_type> BOOL </par_type>
            <par_required> 0 </par_required>
            <par_default> 1 </par_default>
            <par_unit> none </par_unit>
        </input_par_config>
        <input_par_config>
            <par_name> spiralSpacing </par_name>
            <par_desc> line spacing of the spiral path </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
            <par_max> 1.0 </par_max>
            <par_min> 0.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> equalRadius </par_name>
            <par_desc> Convert orientation to linear distance. It standards for the ability that the orientation follows the orientation </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default>0.1 </par_default>
            <par_max> 10.0 </par_max>
            <par_min> 0.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> maxPathJerk </par_name>
            <par_desc> maximum jerk along the path </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m/s^3 </par_unit>
            <par_required> 0 </par_required>
            <par_default>100.0 </par_default>
            <par_max> 10000.0 </par_max>
            <par_min> 90.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> maxCornerAcc </par_name>
            <par_desc> maximum acceleration at corner </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m/s^2 </par_unit>
            <par_required> 0 </par_required>
            <par_default>2.0 </par_default>
            <par_max> 6.0 </par_max>
            <par_min> 0.5 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> maxCornerJerk </par_name>
            <par_desc> maximum jerk at corner </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> m/s^3 </par_unit>
            <par_required> 0 </par_required>
            <par_default>200.0 </par_default>
            <par_max> 10000.0 </par_max>
            <par_min> 10.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> maxForceVel </par_name>
            <par_desc> maximum force transition velocity </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default>100.0 </par_default>
            <par_max> 10000.0 </par_max>
            <par_min> 10.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> maxForceAcc </par_name>
            <par_desc> maximum force transition acceleration </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default>1000.0 </par_default>
            <par_max> 100000.0 </par_max>
            <par_min> 100.0 </par_min>
        </input_par_config>
        <input_par_config>
            <par_name> preferJntPos </par_name>
            <par_desc> preferred target joint configuration. The robot is compliance with Cartesian constraint while reaching this configuration as close as possible.</par_desc>
            <par_type> VEC_7d </par_type>
            <par_unit> deg </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 -40.0 0.0 90.0 0.0 40.0 0.0 </par_default>
            <par_max> 160  130  170  107 170  260  170</par_max>
            <par_min> -160 -130 -170 -154 -170 -80 -170</par_min>
            <par_freedrive> 1 </par_freedrive>
        </input_par_config>
        <input_par_config>
            <par_name> frictionViscousCoeff </par_name>
            <par_desc> "viscous coefficient of friction" </par_desc>
            <par_type> DOUBLE </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> 0.0 </par_default>
        </input_par_config>
        <input_par_config>
            <par_name> motionStiffnessScalePercentage </par_name>
            <par_desc> "motion stiffness scale percentage at local tcp frame, range from 20.0 to 100.0" </par_desc>
            <par_type> VEC_6d </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> 100.0 100.0 100.0 100.0 100.0 100.0</par_default>
        </input_par_config>
        <input_par_config>
            <par_name> targetTolLevel </par_name>
            <par_desc> "tolerance level of target pose accuracy ranged from 1 to 10, where 1 is the smallest tolerence" </par_desc>
            <par_type> INT </par_type>
            <par_unit> none </par_unit>
            <par_required> 0 </par_required>
            <par_default> 3 </par_default>
            <par_max> 10 </par_max>
            <par_min> 1 </par_min>
        </input_par_config>
        <!-- ===================================================================
         state parameter configuration
         ====================================================================-->
        <state_par_config>
            <par_name> reachedTarget </par_name>
            <par_desc> "flag to indicate if the robot has reached target" </par_desc>
            <par_type> BOOL</par_type>
            <par_unit> none </par_unit>
        </state_par_config>
        <state_par_config>
            <par_name> waypointIndex </par_name>
            <par_desc> "Index of the current waypoint the robot just passed. The waypoint index is 0 indexed where the 0th index is the initial pose" </par_desc>
            <par_type> INT </par_type>
            <par_unit> none </par_unit>
        </state_par_config>
        <!-- ===================================================================
                output parameter configuration
         ====================================================================-->
        <!-- ==================================================================
         default transition condition  
          condition_type : trigger condition type    
          lhs_param_name : LHS parameter name. It must be one of the state parameters in primitive  
          lhs_param_category : LHS parameter category. This should be PT_STATE for now 
          rhs_param_name (optional): RHS parameter name. no need to fill this field if rhs is a constant.
          rhs_param_category : RHS parameter category 
          rhs_param_data (optional) : RHS parameter data. 
         ====================================================================-->
        <default_transition_condition>
            <condition_type> EQUAL</condition_type>
            <lhs_param_name>reachedTarget</lhs_param_name>
            <lhs_param_category>PT_STATE</lhs_param_category>
            <rhs_param_category>CONST</rhs_param_category>
            <rhs_param_data>1</rhs_param_data>
        </default_transition_condition>
    </primitive>
</fvr>
